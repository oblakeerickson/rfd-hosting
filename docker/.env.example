# GitHub webhook secret for signature verification
# Generate with: openssl rand -hex 32
GITHUB_WEBHOOK_KEY=your-webhook-secret-here

# PostgreSQL (if using db profile)
DB_PASSWORD=your-secure-password-here

# rfd-site (if using frontend profile)
# Create OAuth client via rfd-cli: rfd-cli sys oauth create
RFD_API_CLIENT_ID=
RFD_API_CLIENT_SECRET=
RFD_API_GITHUB_CALLBACK_URL=https://rfd.yourdomain.com/auth/github/callback
SESSION_SECRET=generate-with-openssl-rand-hex-32
